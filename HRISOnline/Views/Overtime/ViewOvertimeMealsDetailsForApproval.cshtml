@model IEnumerable<HRISOnline.Objects.OvertimeMealsDetails>
@{
    ViewBag.Title = @ViewBag.MyTitle;
}

<html>
    <head>
        <title>List of Filed Missing Punch</title>

          <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
         <script type="text/javascript" src="@Url.Content("https://cdn.datatables.net/1.10.2/js/jquery.dataTables.min.js")"></script>
         <script type="text/javascript" src="@Url.Content("https://cdn.datatables.net/1.10.12/css/dataTables.bootstrap.min.css")"></script>       
         <script>
             $(document).ready(function () {
                 $('#myTable').dataTable();
             });
         </script>
</head>
    <body>

    <p style="float:right;">
         @Html.ActionLink(" Back", "OvertimeMealsForApproval", null, new { @class="btn btn-danger" })
    </p>

        <table id="myTables" class="table table-striped table-bordered">
        <thead>
            <tr style="font-size:12px; text-align:center;">
                   <th style="text-align:center;" >Employee ID</th>
                   <th style="text-align:center;">Employee Name</th>
                   <th style="text-align:center;">Position</th>
            </tr>
        </thead>
        <tbody id="idresulttable">
             @foreach (var item in Model)
             {
                  <tr style="font-size:12px; text-align:center;">
                    <td>@Html.DisplayFor(a => item.intMstEmpPersonal)</td>
                    <td>@Html.DisplayFor(a => item.EmployeeName)</td>
                    <td>@Html.DisplayFor(a => item.PositionName)</td>        
                  </tr>
             }
        </tbody>
    </table>

    </body>
    </html>

