        function PersonalInfo_Add() {

            var EmployeeId = $('#EmployeeId').val();
            var EmployeeName = $('#EmployeeName').val();
            var Email = $('#Email').val();
            var BloodType = $('#BloodType').val();
            var HomePhone = $('#HomePhone').val();
            var MobileNum = $('#Mobile').val();
            var PermanentAdd = $('#PermanentAdd').val();
            var PresentAdd = $('#PresentAdd').val();
            var ProvincialAdd = $('#ProvincialAdd').val();
            var PersonNotify = $('#PersonNotify').val();
            var Relation = $('#Relation').val();
            var ContactNum = $('#ContactNum').val();


            var tempPersonalRecord = {
                "EmployeeId": EmployeeId,
                "EmployeeName": EmployeeName,
                "Email": Email,
                "BloodType" : BloodType,
                "HomeNum": HomePhone,
                "MobileNum": MobileNum,
                "PermanentAdd": PermanentAdd,
                "PresentAdd": PresentAdd,
                "ProvincialAdd": ProvincialAdd,
                "PersonToNofify": PersonNotify,
                "Relation": Relation,
                "ContactNum" : ContactNum
            };

            $.ajax({
                url: '@Url.Action("UpdatePersonalRecord", "EmployeeTwoOhOne")',
                type: 'POST',
                data: JSON.stringify(tempPersonalRecord),
                datatype: 'json',
                contentType: 'application/json',
                success: function (result) {
                    $('#msg1').addClass('active');
                    $('#msg1').removeClass('hide');

                    if (result != null && result.success) {
                        $('#msg1').removeClass('bg-danger');
                        $('#msg1').addClass('bg-success');
                        document.getElementById("UpdatePersonal").disabled = true;
                        $('#msg1').html(result.responseText).show().slideDown(3000).fadeOut(8000);
                        setTimeout('RedirectMe()', 6000);
                    }
                    else {
                        $('#msg1').removeClass('bg-success');
                        $('#msg1').addClass('bg-danger');
                        $('#msg1').html(result.responseText).show().slideDown(3000).fadeOut(8000);                     
                        }
                     }
            });

            $('#myModal1').modal('hide');

        }

