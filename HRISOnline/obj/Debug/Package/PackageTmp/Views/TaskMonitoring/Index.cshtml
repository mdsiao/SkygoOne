    function CashCountCollection() {

        var EmpID = '@Session["intMstEmpPersonal"]';
        var CashCountCollectionSKYGO = $('#ccCollectionSKYGO').val();
        var CashCountAccountabilitySKYGO = $('#ccAccountabilitySKYGO').val();
        var datCollectionDateSKYGO = $('#CollectionDateSKYGO').val();
        var CashCountCollectionSNDC = $('#ccCollectionSNDC').val();
        var CashCountAccountabilitySNDC = $('#ccAccountabilitySNDC').val();
        var datCollectionDateSNDC = $('#CollectionDateSNDC').val();


        var tempCashCountCollection = {
            "EmployeeID" : EmpID,
            "intCashCountCollectionSKYGO": CashCountCollectionSKYGO,
            "intCashCountAccountabilitySKYGO": CashCountAccountabilitySKYGO,
            "datCollectionDateSKYGO": datCollectionDateSKYGO,
            "intCashCountCollectionSNDC": CashCountCollectionSNDC,
            "intCashCountAccountabilitySNDC": CashCountAccountabilitySNDC,
            "datCollectionDateSNDC": datCollectionDateSNDC

        };

        $.ajax({
            url: '@Url.Action("SaveCashCountCollection", "TaskMonitoring")',
            type: 'POST',
            data: JSON.stringify(tempCashCountCollection),
            datatype: 'json',
            contentType: 'application/json',
            success: function (result) {
                $('#SystemMessage').addClass('active');
                $('#SystemMessage').removeClass('hide');
                $('#SystemMessage1').addClass('active');
                $('#SystemMessage1').removeClass('hide');
                $('#loadspinner').show();
                

                if (result != null && result.success) {

                    $('#SystemMessage').removeClass('bg-danger');
                    $('#SystemMessage').addClass('bg-success');
                    $('#SystemMessage1').removeClass('bg-danger');
                    $('#SystemMessage1').addClass('bg-success');

                    setTimeout(function () {
                        $('#loadspinner').hide();
                        $('.modal-title').html("Confirmation");
                        $('.modal-body').html("You have Sucessfully Saved!");
                        $('#SuccessErrorMessage').modal();
                    }, 5000);




                    $('#ccCollectionSKYGO').val('');
                    $('#ccAccountabilitySKYGO').val('');
                    $('#CollectionDateSKYGO').val('');
                    $('#ccCollectionSNDC').val('');
                    $('#ccAccountabilitySNDC').val('');
                    $('#CollectionDateSNDC').val('');

                    location.reload();
                                            
                }
                else {

                    $('#SystemMessage').removeClass('bg-success');
                    $('#SystemMessage').addClass('bg-danger');
                    $('#SystemMessage1').removeClass('bg-success');
                    $('#SystemMessage1').addClass('bg-danger');

                    setTimeout(function () {
                        $('#loadspinner').hide();
                        $('.modal-title').html("Confirmation");
                        $('.modal-body').html("You have Failed to Saved!");
                        $('#SuccessErrorMessage').modal();
                     
                    }, 5000);


                    
                }
            }
        });

        $('#myModal1').modal('hide');
    }

